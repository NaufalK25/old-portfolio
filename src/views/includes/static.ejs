<% if (styles.length !== 0) { %>
<% for (style of styles) { %>
<link rel="stylesheet" href="<%= trailingDotUrl %>css/<%= style %>.css" />
<% } %>
<% } %>

<% if (scripts.length !== 0) { %>
<% for (script of scripts) { %>
<script defer src="<%= trailingDotUrl %>js/<%= script %>.js"></script>
<% } %>
<% } %>
